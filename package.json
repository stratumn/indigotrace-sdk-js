  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.5.2",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.5.0",
    "babelrc-rollup": "^3.0.0",
    "chai": "^4.1.2",
